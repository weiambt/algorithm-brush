package _03二分;

import java.util.*;
import java.math.*;
import java.io.*;

public class _01我在哪 {
	static int INF = 0x3f3f3f3f;
	static int N = 201000 , M = 2*N;
	static int n,m;
	static String s;

	static int check(int len) {
		for(int i=0;i<s.length();i++) {
			
		}
		return 1;
	}
	public static void main(String[] sss) {
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		s = sc.next();
		int l = 1,r = n;
		while(l<r) {
			int mid = l + r >>1;
			if(check(mid)==1) r=mid;
			else l = mid+1;
		}
	}
}
