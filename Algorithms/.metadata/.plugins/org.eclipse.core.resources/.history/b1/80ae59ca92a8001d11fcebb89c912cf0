package cf;
import java.util.*;
import java.math.*;

public class MainB {
		static int INF = 0x3f3f3f3f;
		static int N = 101 , M = 2*N;
		static int[] ans1 = new int[20];
		static int[] ans2 = new int[20];
		static int n,m;

		public static void main(String[] sss) {
			Scanner sc = new Scanner(System.in);
			n = sc.nextInt();
			while(n-->0){
				int x = sc.nextInt();
				int cnt = 0;
				while(x>0) {
					if(x%10%2==0) {
						ans1[cnt] = x%10/2;
						ans2[cnt] = x%10/2;
					}
					else {
						ans1[cnt] = x%10/2;
						ans2[cnt] = x%10/2+1;
					}
					cnt++;
					x/=10;
				}
				int flag = 0;
				for(int i=cnt-1;i>=0;i--) {
					if(flag==0 && ans1[i]==0) {
						flag = 1;
						continue;
					}
					System.out.print(ans1[i]);
				}
				System.out.print(" ");
				flag = 0;
				for(int i=cnt-1;i>=0;i--) {
					if(flag==0 && ans2[i]==0) continue;
					System.out.print(ans2[i]);
				}
				System.out.println();
			}
		}
}
